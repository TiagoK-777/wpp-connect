{
    "extends": ["config:base"],
    "regexManagers": [
      {
        "fileMatch": ["^config\\.yaml$"],
        "matchStrings": [
          "version:\\s+\"(?<currentValue>.*?)\""
        ],
        "datasourceTemplate": "github-releases",
        "depNameTemplate": "wppconnect-team/wppconnect-server"
      }
    ]
  }